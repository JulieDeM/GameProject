html
  head
    title Dashboard
    link(rel='stylesheet', href='/stylesheets/style.css')
    meta(charset='UTF-8')
  body
    h3 #{userInfo.userName}
    h5 #{userInfo.firstName} #{userInfo.lastName}
    for game in gameTypes
      p Game Type: #{game.game_id}
      for stats in gameStats
        if stats.user_game_id === game.id
          p Rating: #{stats.rating}
